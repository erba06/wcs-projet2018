{
  "name": "light-bootstrap-dashboard-react",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.7",
    "bootstrap-daterangepicker": "^3.0.3",
    "chartist": "0.10.1",
    "google-maps": "^3.3.0",
    "htmllint-cli": "0.0.7",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lint": "^1.1.2",
    "loadash": "^1.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-sass": "4.6.1",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "4.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-bootstrap": "0.32.1",
    "react-bootstrap-date-picker": "^5.1.0",
    "react-bootstrap-date-time-picker": "0.0.3",
    "react-bootstrap-daterangepicker": "^4.1.0",
    "react-bootstrap-datetimerangepicker": "^2.0.4",
    "react-chartist": "0.13.1",
    "react-date-picker-cs": "^0.1.28",
    "react-datepicker": "^1.6.0",
    "react-daterange-picker": "^2.0.1",
    "react-datetimepicker-bootstrap": "^1.1.2",
    "react-dom": "16.2.0",
    "react-drop-zone": "^3.0.6",
    "react-google-maps": "9.4.5",
    "react-notification-system": "0.2.17",
    "react-router-dom": "4.2.2",
    "react-s-alert": "^1.4.1",
    "react-scripts": "1.1.1",
    "snazzy": "^8.0.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint:fix": "npm run lint -- --fix",
    "lint:js": "standard | snazzy",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:html",
    "lint:css": "stylelint 'client/style/**/*.css'",
    "lint:html": "htmllint 'client/**/*.html'",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "standard": "^12.0.1",
    "stylelint-config-recommended": "^2.1.0"
  }
}
